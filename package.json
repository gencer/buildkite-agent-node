{
  "name": "buildkite-agent-node",
  "version": "0.0.11-pre.1",
  "description": "Access BuildKite agent commands from NodeJS",
  "main": "dist/index.js",
  "repository": "https://github.com/vbfox/buildkite-agent-node",
  "author": "Julien Roncaglia <julien@roncaglia.fr>",
  "license": "MIT",
  "source": "src/index.ts",
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/node": "^13.13.5",
    "@types/node-fetch": "^2.5.3",
    "@types/uuid": "^7.0",
    "@types/verror": "^1.10.3",
    "microbundle": "^0.12.0-next.9",
    "typescript": "^3.7.2"
  },
  "scripts": {
    "build-lib": "yarn run -s microbundle -f cjs",
    "build-test": "yarn run -s tsc test/test.ts",
    "build": "yarn run -s build-lib && yarn run -s build-test",
    "prepublishOnly": "yarn build-lib",
    "preversion": "yarn build",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "glob": "^7.1.6",
    "node-fetch": "^2.6.0",
    "uuid": "^8.0.0",
    "verror": "^1.10.0"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts"
  ],
  "directories": {
    "lib": "dist"
  },
  "keywords": [
    "buildkite",
    "agent",
    "ci"
  ]
}
